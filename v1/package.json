{
  "name": "omnitweety",
  "version": "0.9.1",
  "description": "Chrome Extension just for tweeting from omnibox",
  "main": "index.js",
  "repository": "https://github.com/yshrsmz/omnitweety",
  "author": "yshrsmz",
  "license": "MIT",
  "scripts": {
    "build": "webpack --progress --colors --config ./webpack.config.dev.js && cpx 'assets/**/*.png' app/assets",
    "build:release": "webpack --progress --colors --config ./webpack.config.release.js && cpx 'assets/**/*.png' app/assets",
    "clean": "rimraf ./app",
    "tsfmt": "tsfmt -r --useTsconfig=./tsconfig.json",
    "tslint:fix": "tslint --fix -c tslint.json 'src/**/*.{ts,tsx}'",
    "remotedev": "remotedev --hostname=localhost --port=8080",
    "test": "tsc -p ./tsconfig.test.json && nyc ava \"app/**/*test.js\""
  },
  "dependencies": {
    "@types/redux-saga": "^0.10.5",
    "axios": "^0.16.1",
    "lodash-es": "^4.17.4",
    "minimist": "^1.2.0",
    "oauth": "^0.9.15",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.15.3",
    "rx": "^4.1.0",
    "twit": "^2.2.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chrome": "^0.0.46",
    "@types/lodash-es": "^4.14.5",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "@types/redux-logger": "^3.0.0",
    "ava": "^0.19.1",
    "cpx": "^1.5.0",
    "dts-gen": "^0.5.6",
    "html-webpack-plugin": "^2.28.0",
    "nyc": "^10.3.2",
    "remote-redux-devtools": "^0.5.10",
    "remotedev-server": "^0.2.2",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.0.3",
    "tslint": "^5.2.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.2",
    "typescript-formatter": "^5.1.3",
    "webpack": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
